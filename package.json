{
	"name": "svelte-commerce",
	"version": "2022.10.04",
	"private": true,
	"scripts": {
		"dev": "vite dev --host --port 3000",
		"build": "vite build",
		"start-node": "node build/index.js",
		"start": "vite preview --host --port 3000",
		"update": "npm exec -- npx ncu -u -p npm i",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"partytown": "partytown copylib static/~partytown",
		"gen:manifest": "node ./generate-manifest.js"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "1.0.0-next.84",
		"@sveltejs/kit": "1.0.0-next.520",
		"@tailwindcss/line-clamp": "^0.4.2",
		"@tailwindcss/typography": "^0.5.7",
		"@types/bcrypt": "^5.0.0",
		"@typescript-eslint/eslint-plugin": "^5.40.1",
		"@typescript-eslint/parser": "^5.40.1",
		"autoprefixer": "^10.4.12",
		"cookie": "^0.5.0",
		"cookie-universal": "^2.2.2",
		"cssnano": "^5.1.13",
		"daisyui": "^2.31.0",
		"eslint": "^8.25.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"konva": "^8.3.13",
		"postcss": "^8.4.18",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.8.0",
		"svelte": "^3.52.0",
		"svelte-carousel": "^1.0.20",
		"svelte-check": "^2.9.2",
		"svelte-confetti": "^1.2.0",
		"svelte-preprocess": "^4.10.7",
		"svelte-select": "^4.4.7",
		"svelte-timeago": "0.1.2",
		"svelte-toasts": "^1.1.2",
		"tailwind-scrollbar": "2.1.0-preview.0",
		"tailwindcss": "^3.2.0",
		"tslib": "^2.4.0",
		"typescript": "~4.8.4",
		"vanilla-lazyload": "^17.8.3",
		"vite": "^3.1.8"
	},
	"dependencies": {
		"@sentry/svelte": "7.16.0",
		"@sentry/tracing": "7.16.0",
		"amazon-s3-uri": "^0.1.1",
		"aws-sdk": "^2.1236.0",
		"bcrypt": "^5.1.0",
		"dayjs": "^1.11.5",
		"hash-it": "^5.0.2"
	},
	"type": "module",
	"volta": {
		"node": "17.4.0",
		"yarn": "1.22.17",
		"pnpm": "6.32.1"
	}
}